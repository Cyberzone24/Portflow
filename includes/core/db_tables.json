{
    "role": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "caption": "VARCHAR(255) NOT NULL",
        "description": "TEXT",
        "PRIMARY KEY": "(uuid)"
    },
    "users": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "role": "UUID REFERENCES role(uuid) NOT NULL",
        "login_provider": "VARCHAR(255) NOT NULL",
        "username": "VARCHAR(255) NOT NULL UNIQUE",
        "password": "VARCHAR(255)",
        "email": "VARCHAR(320) UNIQUE",
        "activation_code": "VARCHAR(255)",
        "tfa_secret": "VARCHAR(255)",
        "notification_provider": "VARCHAR(255)",
        "notification_setting": "INT",
        "settings": "TEXT",
        "ip_address": "VARCHAR(255)",
        "last_login": "TIMESTAMP WITH TIME ZONE",
        "last_login_attempt": "TIMESTAMP WITH TIME ZONE",
        "login_attempts": "INT",
        "created": "TIMESTAMP WITH TIME ZONE",
        "last_changed": "TIMESTAMP WITH TIME ZONE",
        "PRIMARY KEY": "(uuid)"
    },
    "metadata": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "users": "UUID REFERENCES users(uuid)",
        "status": "INT",
        "caption":"VARCHAR(255) NOT NULL",
        "description": "TEXT",
        "specification": "TEXT",
        "tags": "VARCHAR(255)",
        "created": "TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP",
        "changed": "TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP",
        "PRIMARY KEY": "(uuid)"
    },
    "access": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "role": "UUID NOT NULL REFERENCES role(uuid) NOT NULL",
        "resource": "VARCHAR(255) NOT NULL",
        "access_right": "INT",
        "PRIMARY KEY": "(uuid)"
    },
    "location": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "parent_location": "UUID REFERENCES location(uuid)",
        "type": "FLOAT",
        "size": "TEXT",
        "rotation": "TEXT",
        "PRIMARY KEY": "(uuid)"
    },
    "device": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "location": "UUID REFERENCES location(uuid)",
        "mac_address": "VARCHAR(255)",
        "hostname": "VARCHAR(255)",
        "serial": "VARCHAR(255)",
        "asset": "VARCHAR(255)",
        "manufacturer": "VARCHAR(255)",
        "model": "VARCHAR(255)",
        "type": "VARCHAR(255)",
        "anc": "BOOLEAN",
        "position": "FLOAT",
        "rotation": "INT",
        "size": "TEXT",
        "item_group": "UUID",
        "template": "BOOLEAN",
        "PRIMARY KEY": "(uuid)"
    },
    "device_port": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "device": "UUID NOT NULL REFERENCES device(uuid)",
        "speed": "FLOAT",
        "type": "VARCHAR(255)",
        "size": "TEXT",
        "position": "TEXT",
        "rotation": "TEXT",
        "coupling": "BOOLEAN",
        "tags": "VARCHAR(255)",
        "PRIMARY KEY": "(uuid)"
    },
    "connection": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "device_port_source": "UUID NOT NULL REFERENCES device_port(uuid) NOT NULL",
        "device_port_destination": "UUID REFERENCES device_port(uuid)",
        "type": "VARCHAR(255)",
        "length": "FLOAT",
        "crossover": "BOOLEAN",
        "speed": "FLOAT",
        "item_group": "UUID",
        "PRIMARY KEY": "(uuid)"
    },
    "vlan": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "vlan": "INT NOT NULL",
        "PRIMARY KEY": "(uuid)"
    },
    "device_port_vlan": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "device_port": "UUID NOT NULL REFERENCES device_port(uuid) NOT NULL",
        "vlan": "UUID NOT NULL REFERENCES vlan(uuid) NOT NULL",
        "tagged": "BOOLEAN",
        "PRIMARY KEY": "(uuid)"
    }
}