{
    "ports": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "site": "VARCHAR(1020)",
        "room": "VARCHAR(1020)",
        "port": "VARCHAR(1020)",
        "status": "VARCHAR(1020)",
        "speed": "VARCHAR(1020)",
        "device": "VARCHAR(1020)",
        "hostname": "VARCHAR(1020)",
        "vlan_untagged": "VARCHAR(1020)",
        "vlan_tagged": "VARCHAR(1020)",
        "mac": "VARCHAR(1020)",
        "cable_number": "VARCHAR(1020)",
        "panel": "VARCHAR(1020)",
        "switch_name": "VARCHAR(1020)",
        "switch_port": "VARCHAR(1020)",
        "comment": "VARCHAR(1020)",
        "created": "TIMESTAMP WITH TIME ZONE",
        "last_changed": "TIMESTAMP WITH TIME ZONE",
        "tags": "VARCHAR(1020)",
        "PRIMARY KEY": "(uuid)",
        "FOREIGN KEY": "(vlan_untagged) REFERENCES vlans(vlan_untagged)",
        "FOREIGN KEY": "(vlan_tagged) REFERENCES vlans(vlan_tagged)",
        "FOREIGN KEY": "(switch_name) REFERENCES switch(switch_name)"
    },
    "switches": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "switch_name": "VARCHAR(1020)",
        "uplink_ports": "VARCHAR(1020)",
        "vlans": "VARCHAR(1020)",
        "hostname": "VARCHAR(1020)",
        "port": "VARCHAR(1020)",
        "protocol": "VARCHAR(1020)",
        "username": "VARCHAR(1020)",
        "password": "VARCHAR(1020)",
        "comment": "VARCHAR(1020)",
        "created": "TIMESTAMP WITH TIME ZONE",
        "last_changed": "TIMESTAMP WITH TIME ZONE",
        "tags": "VARCHAR(1020)",
        "PRIMARY KEY": "(uuid)"
    },
    "vlans": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "vlan_untagged": "VARCHAR(1020)",
        "vlan_tagged": "VARCHAR(1020)",
        "name": "VARCHAR(1020)",
        "description": "VARCHAR(1020)",
        "created": "TIMESTAMP WITH TIME ZONE",
        "last_changed": "TIMESTAMP WITH TIME ZONE",
        "tags": "VARCHAR(1020)",
        "PRIMARY KEY": "(uuid)"
    },
    "users": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "username": "VARCHAR(1020) NOT NULL UNIQUE",
        "password": "VARCHAR(1020)",
        "email": "VARCHAR(1020) NOT NULL UNIQUE",
        "activation_code": "VARCHAR(1020)",
        "account_level": "INT",
        "tfa": "VARCHAR(1020)",
        "notification_setting": "INT",
        "language": "VARCHAR(1020)",
        "ip_address": "VARCHAR(1020)",
        "last_login": "TIMESTAMP WITH TIME ZONE",
        "last_login_attempt": "TIMESTAMP WITH TIME ZONE",
        "login_attempts": "INT",
        "created": "TIMESTAMP WITH TIME ZONE",
        "last_changed": "TIMESTAMP WITH TIME ZONE",
        "tags": "VARCHAR(1020)",
        "PRIMARY KEY": "(uuid)"
    },
    "logs": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "username": "VARCHAR(1020)",
        "action": "VARCHAR(1020)",
        "timestamp": "TIMESTAMP WITH TIME ZONE",
        "PRIMARY KEY": "(uuid)"
    }
}