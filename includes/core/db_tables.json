{
    "role": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "caption": "VARCHAR(255) NOT NULL",
        "description": "TEXT",
        "PRIMARY KEY": "(uuid)"
    },
    "users": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "role": "UUID REFERENCES role(uuid) NOT NULL",
        "login_provider": "VARCHAR(255) NOT NULL",
        "username": "VARCHAR(255) NOT NULL UNIQUE",
        "password": "VARCHAR(255)",
        "email": "VARCHAR(320) UNIQUE",
        "activation_code": "VARCHAR(255)",
        "tfa_secret": "VARCHAR(255)",
        "notification_provider": "VARCHAR(255)",
        "notification_setting": "INT",
        "settings": "TEXT",
        "ip_address": "VARCHAR(255)",
        "last_login": "TIMESTAMP WITH TIME ZONE",
        "last_login_attempt": "TIMESTAMP WITH TIME ZONE",
        "login_attempts": "INT",
        "created": "TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP",
        "changed": "TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP",
        "PRIMARY KEY": "(uuid)"
    },
    "changelog": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "users": "UUID REFERENCES users(uuid)",
        "operation": "VARCHAR(10) NOT NULL",
        "changed_table": "VARCHAR(255) NOT NULL",
        "changed_row": "UUID NOT NULL",
        "changed_data": "TEXT",
        "changed": "TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP",
        "PRIMARY KEY": "(uuid)"
    },
    "metadata": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "users": "UUID REFERENCES users(uuid)",
        "status": "INT",
        "caption":"VARCHAR(255) NOT NULL",
        "description": "TEXT",
        "specification": "TEXT",
        "tags": "VARCHAR(255)",
        "created": "TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP",
        "changed": "TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP",
        "PRIMARY KEY": "(uuid)"
    },
    "journal": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "reference": "UUID NOT NULL",
        "PRIMARY KEY": "(uuid)"
    },
    "access": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "role": "UUID NOT NULL REFERENCES role(uuid) NOT NULL",
        "resource": "VARCHAR(255) NOT NULL",
        "access_right": "INT",
        "PRIMARY KEY": "(uuid)"
    },
    "location": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "parent_location": "UUID REFERENCES location(uuid)",
        "type": "FLOAT",
        "size": "TEXT",
        "rotation": "TEXT",
        "PRIMARY KEY": "(uuid)"
    },
    "device": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "location": "UUID REFERENCES location(uuid)",
        "expected_location": "UUID REFERENCES location(uuid)",
        "mac_address": "VARCHAR(255)",
        "hostname": "VARCHAR(255)",
        "expected_hostname": "VARCHAR(255)",
        "serial": "VARCHAR(255)",
        "asset": "VARCHAR(255)",
        "manufacturer": "VARCHAR(255)",
        "model": "VARCHAR(255)",
        "type": "VARCHAR(255)",
        "anc": "BOOLEAN",
        "position": "FLOAT",
        "rotation": "INT",
        "size": "TEXT",
        "item_group": "UUID",
        "template": "BOOLEAN DEFAULT FALSE",
        "PRIMARY KEY": "(uuid)"
    },
    "ip_range": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "ip_range": "VARCHAR(255)",
        "subnet": "INT",
        "gateway": "VARCHAR(255)",
        "broadcast": "VARCHAR(255)",
        "dns_server": "VARCHAR(255)",
        "dns_zone": "VARCHAR(255)",
        "dhcp_server": "VARCHAR(255)",
        "PRIMARY KEY": "(uuid)"
    },
    "vlan": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "ip_range": "UUID REFERENCES ip_range(uuid)",
        "vlan": "INT NOT NULL",
        "PRIMARY KEY": "(uuid)"
    },
    "device_port": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "device": "UUID NOT NULL REFERENCES device(uuid)",
        "vlan": "UUID REFERENCES vlan(uuid)",
        "expected_vlan": "UUID REFERENCES vlan(uuid)",
        "vlan_tagged": "BOOLEAN DEFAULT FALSE",
        "expected_vlan_tagged": "BOOLEAN DEFAULT FALSE",
        "ip": "VARCHAR(255)",
        "expected_ip": "VARCHAR(255)",
        "dhcp_address": "BOOLEAN DEFAULT FALSE",
        "expected_dhcp_address": "BOOLEAN DEFAULT FALSE",
        "speed": "FLOAT",
        "expected_speed": "FLOAT",
        "type": "VARCHAR(255)",
        "size": "TEXT",
        "position": "TEXT",
        "rotation": "TEXT",
        "coupling": "BOOLEAN DEFAULT FALSE",
        "PRIMARY KEY": "(uuid)"
    },
    "device_lifecycle": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "device": "UUID REFERENCES device(uuid) NOT NULL",
        "value_date": "DATE",
        "PRIMARY KEY": "(uuid)"
    },
    "connection": {
        "uuid": "UUID DEFAULT gen_random_uuid() NOT NULL",
        "metadata": "UUID REFERENCES metadata(uuid)",
        "device_port_source": "UUID NOT NULL REFERENCES device_port(uuid)",
        "expected_device_port_source": "UUID NOT NULL REFERENCES device_port(uuid)",
        "device_port_destination": "UUID REFERENCES device_port(uuid)",
        "expected_device_port_destination": "UUID REFERENCES device_port(uuid)",
        "type": "VARCHAR(255)",
        "length": "FLOAT",
        "crossover": "BOOLEAN DEFAULT FALSE",
        "speed": "FLOAT",
        "item_group": "UUID",
        "PRIMARY KEY": "(uuid)"
    }
}